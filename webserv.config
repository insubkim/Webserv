#asd
http {
  server {
    listen  80; 
    server_name localhost;
    location / {
        index  index.html;
        root html;
        autoindex on;
        error_page 404 404.html;
    }
    location ~ .html {
      root html;
      error_page 404 1index.html;
      cgi_root cgi/cgi_tester;
    }
  }
  
    server {
    listen  81; 
    server_name localhost;

    location / {
        index  404.html;
        root html;
        autoindex on;
        error_page 404 1index.html;
        error_page 500 2index.html;
    }
  }
}